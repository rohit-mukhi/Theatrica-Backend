{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/reviews/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "https://theatrica.netlify.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, X-Requested-With, Accept"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
